<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.naveen.waterfalls"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/maincon"
        android:label="@string/app_name"
        android:theme="@style/Fullscreen" >
        <activity
            android:name="com.naveen.waterfalls.Mainfalls"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.naveen.waterfalls.FallsDetails"
            android:label="@string/title_activity_falls_details"
            android:screenOrientation="portrait"  >
        </activity>
        <activity
            android:name="com.naveen.waterfalls.FullScreen"
            android:label="@string/title_activity_full_screen"
            android:screenOrientation="portrait"  >
        </activity>
    </application>

</manifest>